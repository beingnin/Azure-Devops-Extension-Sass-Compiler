{
    "manifestVersion": 1,
    "id": "process-sass-files",
    "name": "Process sass files",
    "version": "0.0.9",
    "publisher": "beingnin",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "description": "Tools for compiling sass to css and add vendor prefixes",
    "categories": [
        "Azure Pipelines"
    ],
    "tags":["sass","scss","css", "compile css","compile sass","precompile","postcompile"],
    "icons": {
        "default": "images/extension-icon.png"        
    },
    "content": {
        "details": {
            "path": "overview.md"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/beingnin/Azure-Devops-Extension-Sass-Compiler"
    },
    "branding": {
        "color": "rgb(217, 26, 134)",
        "theme": "light"
    },    
    "galleryFlags": [
        "Public","Preview"
    ],    
    "files": [
        {
            "path": "buildAndReleaseTask"
        }
    ],
    "contributions": [
        {
            "id": "process-sass-files",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "buildAndReleaseTask"
            }
        }
    ]
}